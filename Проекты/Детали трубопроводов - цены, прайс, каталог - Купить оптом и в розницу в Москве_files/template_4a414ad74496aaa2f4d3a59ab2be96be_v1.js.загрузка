; /* Start:"a:4:{s:4:"full";s:48:"/local/templates/bas/js/script.js?16297971081305";s:6:"source";s:33:"/local/templates/bas/js/script.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
$(function(){

	$('#modal-order').on('show.bs.modal', function (event) {

		var btn = $(event.relatedTarget);

		var recipient = $(btn).data('title');

		$(this).find('.modal-header').text(recipient);

		var txt = $(btn).parents('.cartBlock').find('.cartTitle').text();

		if (txt) $(this).find('.modal-body textarea').val(txt);
		else $(this).find('.modal-body textarea').val('');
	});

	$('#modal-quick').on('show.bs.modal', function (event) {

		var btn = $(event.relatedTarget);
		var block = $(btn).parents('.cartBlock');
		var count = $(block).find('.cartCount input');

		var productTitle = $(block).find('.cartTitle').text();

		$(this).find('.link-action').text(productTitle);

		$(this).find('input[type="number"]').attr('step', $(count).attr('step')).attr('min', $(count).attr('min')).val($(count).val());

		$(this).find('input.product-id').val($(btn).data('product-id'));

		$(this).find('.price_replace').html($(block).find('.cartPrice').html());

		$(this).find('.price_replace').attr('data-price', $(block).find('.cartPrice').data('price'));

		$(this).find('.sum_replace').html($(block).find('.cartPriceSum').html());

		$(this).find('.quick-order__image').attr('src', $(block).find('.cartPict').val());

		$('.spin').spin();

		App.prepareGoalData({
			title: productTitle
		});
	});
});
/* End */
