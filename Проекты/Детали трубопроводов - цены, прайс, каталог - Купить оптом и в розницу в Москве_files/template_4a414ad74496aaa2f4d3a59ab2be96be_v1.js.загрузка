/**
 * Globals
 */

var App = {};

App.EVENT_COMPONENT_CLOSE = 'event-close-other-components';
App.EVENT_MMENU_OPEN = 'event-mobile-menu-open';
App.DATA_KEY_SPIN = 'dpr.spin';

App.triplets = function (str) {
    // \u202f — неразрывный узкий пробел
    return str.toString().replace(/(\d)(?=(\d\d\d)+([^\d]|$))/g, '$1\u202f');
};

function closeOtherPopupComponents(currentUid) {
    $(document).trigger(App.EVENT_COMPONENT_CLOSE, currentUid || null);
}

/**
 * Media breakpoints
 */

(function () {
    var baseRem = 16; // Эквивалентно установленнному в CSS

    function toRem(px) {
        return (px / baseRem) + 'rem';
    }

    App.mq = {
        // Breakpoints constants
        XXS: 0,
        XS : 1,
        SM : 2,
        MD : 3,
        LG : 4,
        XL : 5,
        WD : 6,

        // Breakpoints values (так же, как в CSS)
        BPS: [0, 425, 576, 768, 992, 1200, 1400],
    };

    // Mobile menu toggle breakpoint index
    App.mq.MBP = App.mq.SM;

    App.mq.list = ['(max-width: ' + toRem(App.mq.BPS[1] - 1) + ')'];

    for (var i = 1; i <= App.mq.BPS.length - 2; i++) {
        var mq = '(min-width: ' + toRem(App.mq.BPS[i]) + ') and (max-width: ' + toRem(App.mq.BPS[i + 1] - 1) + ')';
        App.mq.list.push(mq);
    }

    App.mq.list.push('(min-width: ' + toRem(App.mq.BPS[App.mq.BPS.length - 1]) + ')');
})();

(function () {
    function MediaQueryManager() {
        this.handlersIn = {};
        this.handlersOut = {};
        this.list = App.mq.list.map((expr, index) => {
            const mediaQuery = matchMedia(expr);
            let handle = e => {
                const key = e.matches ? 'getHandlersIn' : 'getHandlersOut';
                this[key](index).forEach(h => {
                    typeof h === 'function' && h(expr);
                });
            };
            mediaQuery.addListener(handle);
            handle(mediaQuery);

            return {
                expr,
                mediaQuery,
                handle,
            };
        });

    }

    MediaQueryManager.prototype.addListener = function (mqIndex, callbackIn, callbackOut) {
        if (!this.handlersIn[mqIndex]) this.handlersIn[mqIndex] = [];
        if (!this.handlersOut[mqIndex]) this.handlersOut[mqIndex] = [];
        typeof callbackIn === 'function' && this.handlersIn[mqIndex].push(callbackIn);
        typeof callbackOut === 'function' && this.handlersOut[mqIndex].push(callbackOut);
    };

    MediaQueryManager.prototype.getHandlersIn = function (mqIndex) {
        if (this.handlersIn[mqIndex]) {
            return this.handlersIn[mqIndex];
        }
        return [];
    };

    MediaQueryManager.prototype.getHandlersOut = function (mqIndex) {
        if (this.handlersOut[mqIndex]) {
            return this.handlersOut[mqIndex];
        }
        return [];
    };

    MediaQueryManager.prototype.callOnLoad = function () {
        this.list.forEach(item => item.handle(item.mediaQuery));
    };

    App.mediaQueryManager = new MediaQueryManager();
})();

/**
 * Yandex metrika
 * @external ym
 */

(function () {

    var goals = {
        ModalCallbackMessage: 'call-back',     // перезвонить мне
        ModalOrderMessage   : 'order-header',  // форма отправить заявку в шапке
        AboutPageMessage    : 'send-letter',   // форма обратной связи
        QuickOrderMessage   : 'fast-order',    // быстрый заказ в карточке товара
        BASKET              : 'order-basket',  // корзина, факт отправки заказа (оформление)
    };

    var storedData = null;

    App.YANDEX_COUNTER_ID = 51613268;

    App.prepareGoalData = function (params) {
        storedData = params;
    };

    App.formReachGoal = function (formId) {

        if (goals.hasOwnProperty(formId)) {

            var target = goals[formId];

            var params = {
                url: document.location.href,
            };

            if (storedData) {
                if (storedData.title) params.title = storedData.title;
            }

            ym(this.YANDEX_COUNTER_ID, 'reachGoal', target, params);

            storedData = null;

            console.info(`Достигнута цель: «${target}»`, params);

        } else {
            console.warn(`Неопознанная цель: «${formId}»`);
        }
    };

    $('#modal-order').on('hide.bs.modal', function () { storedData = null; });
    $('#modal-quick').on('hide.bs.modal', function () { storedData = null; });

})();

/**
 * Selector
 *
 * Markup example:
 *
 * <div class="selector c-selector isOpen isSelected">
 *     <div title="" class="selector__toggler">
 *         <div class="selector__title">Диаметр</div>
 *         <div class="selector__indicator"></div>
 *         <div role="button" title="Сбросить выбор" class="selector__reset"><span></span></div>
 *     </div>
 *     <div class="selector__dropdown">
 *         <div class="selector__options">
 *             <div class="">
 *                 <div class="controls-group">
 *                     <label class="checkbox">
 *                         <input type="checkbox" class="checkbox__element">
 *                         <span class="checkbox__indicator"></span>
 *                         <span class="checkbox__label">Item 1</span>
 *                     </label>
 *                     <label class="checkbox">
 *                         <input type="checkbox" class="checkbox__element">
 *                         <span class="checkbox__indicator"></span>
 *                         <span class="checkbox__label">Item 2</span>
 *                     </label>
 *                 </div>
 *             </div>
 *         </div>
 *         <div class="selector__controls">
 *             <button class="btn btn_primary-o btn_size_small">Применить</button>
 *         </div>
 *     </div>
 * </div>
 */

(function($) {
    var globalElementsCounter = 1;

    var isOpenClass     = 'isOpen';
    var isSelectedClass = 'isSelected';

    function isSelected($checkboxes) {
        var selected = false;
        $checkboxes.each(function(i, el) {
            if (el.checked) selected = true;
        });
        return selected;
    }

    function SelectorControl(el, settings) {
        var _this        = this;
        this.uid         = globalElementsCounter++;
        this.$el         = $(el);
        this.$toggler    = $('.selector__toggler', this.$el);
        this.$btnApply   = $('.selector__controls .btn', this.$el);
        this.$btnReset   = $('.selector__reset', this.$el);
        this.$checkboxes = $('input[type="checkbox"]', this.$el);

        this.$toggler.on('click', function() {
            _this.$el.toggleClass(isOpenClass);
            if (_this.$el.hasClass(isOpenClass)) {
                $(document).trigger(settings.EVENT_COMPONENT_CLOSE, [{uid: _this.uid}]);
            }
        });

        this.$checkboxes.on('change', function() {
            _this.$el.toggleClass(isSelectedClass, isSelected(_this.$checkboxes));
        });

        this.$btnApply.on('click', function() {
            _this.$el.removeClass(isOpenClass);
            if (typeof settings.onApply === 'function') {
                settings.onApply.apply(el);
            }
        });

/*        this.$btnReset.on('click', function() {
            _this.clear();
            _this.$el.removeClass(isOpenClass);
            if (typeof settings.onReset === 'function') {
                settings.onReset.apply(el);
            }
        }); */

        $(document).on(settings.EVENT_COMPONENT_CLOSE, function(event, payload) {
            if (!payload) _this.close();
            if (payload && payload.uid && payload.uid !== _this.uid) _this.close();
        });
    }

    SelectorControl.prototype.clear = function() {
        this.$checkboxes.each(function(i, el) {
            el.checked = false;
        });
        this.$el.removeClass(isSelectedClass);
    };

    SelectorControl.prototype.close = function() {
        this.$el.removeClass(isOpenClass);
    };

    var init = function(options) {
        var settings = $.extend({
            EVENT_COMPONENT_CLOSE: (window.App && window.App.EVENT_COMPONENT_CLOSE)
                ? window.App.EVENT_COMPONENT_CLOSE
                : 'event-close',
            onApply              : null,
            onReset              : null,
        }, options);

        return this.each(function() {
            var $this = $(this);
            $this.data('selector-control', new SelectorControl(this, settings));
        });
    };

    jQuery.fn.selectorControl = function(method) {

        if (SelectorControl.prototype.hasOwnProperty(method)) {
            return this.each(function() {
                var selector = $(this).data('selector-control');
                if (selector) {
                    selector[method]();
                }
            });
        } else if (typeof method === 'object' || !method) {
            return init.apply(this, arguments);
        } else {
            $.error('Undefined method: ' + method + ' in jQuery.selectorControl');
        }

    };
})(jQuery);

/**
 * Поле выбора количества
 */

(function() {
    var DATA_KEY       = App.DATA_KEY_SPIN;
    var REPEAT_DELAY_1 = 300;
    var REPEAT_DELAY_2 = 1500;
    var REPEAT_DELAY_3 = 3000;
    var REPEAT_SPEED_1 = 200;
    var REPEAT_SPEED_2 = 100;
    var REPEAT_SPEED_3 = 10;

    var timeout, interval;

    function getIntAttr($el, attr, defaultValue) {
        return parseInt($el.attr(attr)) || defaultValue;
    }

    function getIntValue($el, defaultValue) {
        return parseInt($el.val()) || defaultValue;
    }

    function repeatStop() {
        clearTimeout(timeout);
        clearInterval(interval);
    }

    function Spin($spin, $input) {
        var _this = this;

        this.$spin   = $spin;
        this.$input  = $input;
        this.options = {min: 1, max: Infinity, step: 1};

        function updateParams() {
            _this.options.min  = getIntAttr(_this.$input, 'min', 1);
            _this.options.max  = getIntAttr(_this.$input, 'max', Infinity);
            _this.options.step = getIntAttr(_this.$input, 'step', 1);
        }

        updateParams();

        this.$input.on('input', function() {
            var value = getIntValue(_this.$input);
            if (typeof value === 'undefined') {
                _this.$input.val(_this.options.min);
                _this.$spin.trigger('change', [_this.options.min]);
            } else {
                _this.$spin.trigger('change', [value]);
            }
        });

        this.update = function() {
            updateParams();
        };

        this.increment = function() {
            var value = getIntValue(this.$input, this.options.min);
            value     = Math.min(value + this.options.step, this.options.max);
            this.$input.val(value);
            this.$spin.trigger('change', [value]);
        };

        this.decrement = function() {
            var value = getIntValue(this.$input, this.options.min);
            value     = Math.max(value - this.options.step, this.options.min);
            this.$input.val(value);
            this.$spin.trigger('change', [value]);
        };

        this.repeatStart = function(func) {
            if (func === 'increment' || func === 'decrement') {
                clearTimeout(timeout);
                // Первая скорость
                timeout = setTimeout(function() {
                    interval = setInterval(function() {_this[func]();}, REPEAT_SPEED_1);

                    // Вторая скорость
                    timeout = setTimeout(function() {
                        clearInterval(interval);
                        interval = setInterval(function() {_this[func]();}, REPEAT_SPEED_2);

                        // Третья скорость
                        timeout = setTimeout(function() {
                            clearInterval(interval);
                            interval = setInterval(function() {_this[func]();}, REPEAT_SPEED_3);
                        }, REPEAT_DELAY_3);
                    }, REPEAT_DELAY_2);
                }, REPEAT_DELAY_1);
            }
        };

        this.getOption = function(opt) {
            if (typeof opt === 'undefined' || !opt) {
                return this.options;
            }

            if (this.options.hasOwnProperty(opt)) {
                return this.options[opt];
            } else {
                return null;
            }
        };
    }

    $(document).on('mouseup', repeatStop);

    jQuery.fn.spin = function() {
        return this.each(function() {
            var $this         = $(this);
            var $btnIncrement = $('.spin__btn_increase', $this);
            var $btnDecrement = $('.spin__btn_decrease', $this);
            var $input        = $('.spin__input', $this);

            var spin = $this.data(DATA_KEY);

            if (!spin) {

                spin = new Spin($this, $input);

                $btnIncrement.on('click', function() { spin.increment(); });
                $btnDecrement.on('click', function() { spin.decrement(); });
                $btnIncrement.on('mousedown', function() { spin.repeatStart('increment'); });
                $btnDecrement.on('mousedown', function() { spin.repeatStart('decrement'); });

                $this.data(DATA_KEY, spin);
            } else {
                spin.update();
            }
        });
    };
})();

/**
 * Поле прикрепления файла
 */
jQuery.fn.uploadbox = function() {
    return this.each(function() {

        var $this          = $(this);
        var pickedFileName = '';
        var pickedFileSize = '';
        var fieldName;

        var $label    = $('.uploadbox__label', $this);
        var $file     = $('.uploadbox__file', $this);
        var $filename = $('.uploadbox__filename', $this);
        var $clear    = $('.uploadbox__clear', $this);

        function onChange(e) {
            fieldName = e.target.name;
            if (e.target.files.length && e.target.files[0].name) {
                var pickedFile = e.target.files[0];
                pickedFileName = pickedFile.name;

                var nBytes     = pickedFile.size;
                var aMultiples = ['KB', 'MB', 'GB', 'TB', 'PB', 'EB', 'ZB', 'YB'];
                for (var nMultiple = 0, nApprox = nBytes / 1024; nApprox > 1; nApprox /= 1024, nMultiple++) {
                    pickedFileSize = nApprox.toFixed(1) + ' ' + aMultiples[nMultiple];
                }

                $label.hide();
                $file.show();
                $filename.html(pickedFileName + ' <small>(' + pickedFileSize + ')</small>');
            }
        }

        $this.find('input[type="file"]').on('change', onChange);

        $clear.on('click', function() {
            $this.find('input[type="file"]').remove();
            var $newField = $('<input>', {
                type : 'file',
                class: 'uploadbox__input',
                name : fieldName,
            });
            $newField.on('change', onChange);
            $newField.appendTo($label);
            $label.show();
            $file.hide();
        });
    });
};

/**
 * Карусельки
 */

(function() {
    var defaultPreset = 'horizontal';

    var tinySliderPresets = {
        horizontal: {
            loop      : false,
            mouseDrag : true,
            items     : 1,
            slideBy   : 'page',
            speed     : 900,
            nav       : false,
            responsive: {
                [App.mq.BPS[App.mq.XXS]]: {items: 1},
                [App.mq.BPS[App.mq.XS]] : {items: 2},
                [App.mq.BPS[App.mq.MD]] : {items: 3},
                [App.mq.BPS[App.mq.LG]] : {items: 4},
                [App.mq.BPS[App.mq.XL]] : {items: 5},
            },
        },
        vertical  : {
            axis      : 'vertical',
            loop      : true,
            // mouseDrag : true,
            items     : 2,
            slideBy   : 'page',
            speed     : 900,
            nav       : false,
            responsive: {
                [App.mq.BPS[App.mq.XXS]]: {items: 2},
                [App.mq.BPS[App.mq.MD]] : {items: 3},
                [App.mq.BPS[App.mq.LG]] : {items: 4},
                [App.mq.BPS[App.mq.XL]] : {items: 5},
            },
        },
        logotypes : {
            loop      : false,
            mouseDrag : true,
            items     : 1,
            slideBy   : 'page',
            speed     : 900,
            nav       : false,
            responsive: {
                [App.mq.BPS[App.mq.XXS]]: {items: 1},
                [App.mq.BPS[App.mq.SM]] : {items: 2},
                [App.mq.BPS[App.mq.MD]] : {items: 3},
                [App.mq.BPS[App.mq.LG]] : {items: 4},
                [App.mq.BPS[App.mq.XL]] : {items: 5},
            },
        },
    };

    $('.carousel').each(function(i, el) {
        var presetName = el.dataset.preset || defaultPreset;
        if (!tinySliderPresets.hasOwnProperty(presetName)) presetName = defaultPreset;

        var carouselNode = el.querySelector('.carousel__carousel');
        var controlsNode = el.querySelector('.nav-buttons');

        var sliderOptions = $.extend(tinySliderPresets[presetName], {
            container        : carouselNode,
            controlsContainer: controlsNode,
        });

        tns(sliderOptions);

        // Фикс глюка в вертикальных карусельках
        // принудительно рассылаем resize event, чтобы карусель правильно пересчитала свою высоту
        setTimeout(function() {
            document.dispatchEvent(
                new Event('resize', {bubbles: true, cancelable: true}),
            );
        }, 100);
    });
})();

/**
 * Добавить в корзину (уведомление онли)
 */

function cartNotify(title, link) {
    clearTimeout(cartNotify.timeout);
    var $el              = $('#cart-notify');
    var $titleLink       = $('.cart-notify__title', $el);
    var autoCloseTimeout = parseInt($el.data('auto-close'));

    $titleLink.text(title);
    $titleLink.attr('href', link);

    $el.addClass('cart-notify_open');

    if (!isNaN(autoCloseTimeout) && autoCloseTimeout > 0) {
        cartNotify.timeout = setTimeout(function() {
            $el.removeClass('cart-notify_open');
        }, autoCloseTimeout);
    }
}

cartNotify.timeout = null;

$('.cart-notify__close, .quick-order-cancel').on('click', function(e) {
    e.preventDefault();
    $('#cart-notify').removeClass('cart-notify_open');
});

/**
 * Фиксирующаяся шапка сайта
 */
(function() {
    var $fixedBar   = $('#fixedBar');
    var $siteHeader = $('#siteHeader');

    function toggle() {
        var isFixed = window.pageYOffset > $siteHeader[0].clientHeight;
        $fixedBar.toggleClass('FixedBarIsFixed', isFixed);
    }

    if ($siteHeader.length && $fixedBar.length) {
        $(window).on('scroll', toggle);
        toggle();
    }
})();

/**
 * Галерея превьюшек товара в карточке
 */

(function() {
    $('.gallery').each(function(i, el) {
        var elGallery  = el.querySelector('.gallery__gallery');
        var elThumbs   = el.querySelector('.gallery-thumbs__slider');
        var hasGallery = elGallery !== null;
        var hasThumbs  = elThumbs !== null;

        if (!hasGallery) {
            console.warn('Галерея отсутствует');
            return;
        }

        var galleryConfig = {
            container: el.querySelector('.gallery__gallery'),
            mode     : 'gallery',
            loop     : false,
            mouseDrag: hasThumbs,
            items    : 1,
            slideBy  : 1,
            speed    : 900,
            nav      : hasThumbs,
            controls : false,
        };

        if (hasThumbs) {
            galleryConfig.prevButton      = el.querySelector('.gallery-thumbs__button_prev');
            galleryConfig.nextButton      = el.querySelector('.gallery-thumbs__button_next');
            galleryConfig.navContainer    = el.querySelector('.gallery-thumbs__slider');
            galleryConfig.navAsThumbnails = true;
        }

        tns(galleryConfig);

        if (hasThumbs) {
            tns({
                container : el.querySelector('.gallery-thumbs__slider'),
                loop      : false,
                mouseDrag : true,
                items     : 3,
                slideBy   : 1,
                speed     : 900,
                prevButton: el.querySelector('.gallery-thumbs__button_prev'),
                nextButton: el.querySelector('.gallery-thumbs__button_next'),
                nav       : false,
            });
        }
    });
})();

/**
 * Инпуты
 */
(function() {
    // Второй плейсхолдер, для заполненного поля
    $('.inputbox').each(function(i, el) {
        $(el)
        .find('.inputbox__placeholder')
        .clone()
        .removeClass('inputbox__placeholder')
        .addClass('inputbox__placeholder-alt')
        .appendTo(el);
    });

    // Переключение класса для заполненного поля
    $(document).on('input', '.inputbox', function(ev) {
        $(ev.target).closest('.inputbox').toggleClass('inputbox_filled', !!ev.target.value);
    });
})();

