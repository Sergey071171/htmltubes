hamburgerButton.default({
    onOpen : () => {
        document.body.classList.add('locked-mobile');
        document.body.classList.add('show-mobile-menu');
        $(document).trigger(App.EVENT_MMENU_OPEN);
    },
    onClose: () => {
        document.body.classList.remove('show-mobile-menu');
        if (!document.body.classList.contains('mobile-contacts-is-open')) {
            document.body.classList.remove('locked-mobile');
        }
    },
});

$('.header .btn_contacts').on('click', function() {
    // Не менять на toggle()
    if (!document.body.classList.contains('mobile-contacts-is-open')) {
        document.body.classList.add('locked-mobile');
        document.body.classList.add('mobile-contacts-is-open');
    } else {
        document.body.classList.remove('mobile-contacts-is-open');
        if (!document.body.classList.contains('show-mobile-menu')) {
            document.body.classList.remove('locked-mobile');
        }
    }
});


/**
 * Подсчёт цен в каталоге
 */
$(function() {
    $(document).on('change', '.cartCount', function(ev, val) {

        if (typeof val === 'undefined') return;

        var $spin  = $(ev.target);
        var $block = $spin.closest('.cartBlock');
        if (!$block.length) return;

        try {

            var $price = $block.find('.cartPrice');
            var $sum   = $block.find('.cartPriceSum').find('.price__value');

            var price = +$price.data('price').toString().replace(/\s/, '');
            if (!price) {
                console.warn('Атрибут data-price отсутствует или имеет некорректное значение (.cartPrice)');
                return;
            }

            var sum       = price * val;
            var sumString = App.triplets(sum.toFixed());

            $sum.text(sumString);

        } catch (e) {
            console.error(e.message);
        }
    });
});

// Запустить в конце всего кода
App.mediaQueryManager.callOnLoad();

