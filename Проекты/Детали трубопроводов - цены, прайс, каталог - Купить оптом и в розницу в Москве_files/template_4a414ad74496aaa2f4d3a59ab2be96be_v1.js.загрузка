; /* Start:"a:4:{s:4:"full";s:72:"/local/components/bas/bas.cart.add/templates/bas/script.js?1629797108622";s:6:"source";s:58:"/local/components/bas/bas.cart.add/templates/bas/script.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
$(function(){

	$('body').delegate('.cartBtn', 'click', function(){

		BX.showWait();

		var intCount = $(this).parents('.cartBlock').find('.cartCount input').val();
		var intId = $(this).attr('data-product-id');

		var elem = $(this).parents('.cartBlock').find('.cartTitle');

		var strTitle = $(elem).text();
		var strLink = $(elem).attr('href');

		$.ajax({
			url: '/',
			data: {
				productId: intId,
				productCount: intCount,
				ajax_cart_add: 'Y'
			},
			success: function( res ){

				$('.cartPlase').html(res);

				cartNotify(strTitle, strLink);

				BX.closeWait();
			},
			dataType: 'html'
		});
	});
});
/* End */
