(function () {

    $('.mobile-filter-button').on('click', function () {
        $('.mobile-filter-panel').addClass('isShow');
        $('body').addClass('locked');
    });

    $('.mobile-filter-panel .btn-close').on('click', function () {
        $('.mobile-filter-panel').removeClass('isShow');
        $('body').removeClass('locked');
    });

})();

/**
 * Mobile Main menu
 */

(function() {
    var TAG_BTN = '<button/>';
    var TAG_DIV = '<div/>';
    var TAG_UL  = '<ul/>';
    var TAG_LI  = '<li/>';
    var TAG_A   = '<a/>';

    function striptags(value) {
        return value.toString().replace(/<\/?[^>]+>/gi, ' ');
    }

    function getMenuRecursive(menu) {
        if (!menu || menu.tagName.toLowerCase() !== 'ul') {
            console.warn('Element not UL:', menu);
            return null;
        }

        let brunch = [];

        Array.prototype.forEach.call(menu.children, menuItem => {
            var link         = menuItem.querySelector('a');
            var icon         = menuItem.querySelector('.ci');
            var childrenMenu = menuItem.querySelector('ul');

            // for (let i = 0; i < menuItem.children.length; i++) {
            //     let el = menuItem.children[i], elTagName = el.tagName.toLowerCase();
            //     if (elTagName === 'a') link = el;
            //     if (elTagName === 'ul') childrenMenu = el;
            // }

            if (link) {
                var text = link.innerHTML;
                if (link.children.length) {
                    text = link.querySelector('span').innerHTML;
                }

                var brunchItem = {
                    text,
                    href    : link.getAttribute('href'),
                    icon    : null,
                    children: null,
                };

                if (icon) brunchItem.icon = icon.dataset.icon || null;
                if (childrenMenu) brunchItem.children = getMenuRecursive(childrenMenu);

                brunch.push(brunchItem);
            }
        });

        return brunch;
    }

    var mainmenu = getMenuRecursive(document.getElementById('mainmenu'));
    if (!mainmenu) {
        console.warn('Mainmenu not found: #mainmenu');
        return;
    }

    var catMenu  = getMenuRecursive(document.getElementById('navCatalog'));
    var menulist = (catMenu ? [
        {
            text    : 'Каталог',
            href    : '#',
            icon    : null,
            children: catMenu,
        },
    ] : []).concat(mainmenu);

    var openLevels = [];
    var animated   = false;
    var movedUp    = false;

    function containerStyles(levelsCount) {
        return {width: (100 * (levelsCount + 1)) + '%'};
    }

    function menuStyles(levelsCount) {
        var pxContWidth = $container.width();
        var percent     = 100 / (levelsCount + 1);
        var pixel       = pxContWidth / 100 * percent;
        return {
            // width: (percent) + '%',
            width: (pixel) + 'px',
        };
    }

    function updateStyles() {
        $container.css(containerStyles(openLevels.length));
        $menu.find('.mmenu').css(menuStyles(openLevels.length));
    }

    function slide() {
        let width     = (100 / (openLevels.length + 1));
        let translate = (100 / (openLevels.length + 1)) * -openLevels.length;

        setTimeout(function() {
            $container.css({
                transform: 'translateX(' + (translate + width) + '%)',
            });

            setTimeout(function() {
                $container.addClass('on');
                $container.css({
                    transform: 'translateX(' + (translate) + '%)',
                });
            }, 1);
        }, 1);
    }

    function deepDive(items, title) {
        animated = true;
        openLevels.push({
            title: title,
            items: items,
        });

        render(items, title);
        slide();
    }

    function render(items, title) {
        title   = title || null;
        var $ul = $(TAG_UL, {class: 'mmenu'});

        if (title) {
            var $backItem = $(TAG_LI, {class: 'mmenu__item mmenu__header'});
            var $backBtn  = $(TAG_BTN, {class: 'mmenu__button2', text: title});
            $backBtn.appendTo($backItem);
            $backItem.appendTo($ul);
        }

        for (var i = 0; i < items.length; i++) {
            var item = items[i];

            var $li = $(TAG_LI, {class: 'mmenu__item'});

            var $link = $(TAG_A, {
                class: 'mmenu__link',
                href : item.href,
                text : item.text,
            });
            $link.appendTo($li);

            if (item.children && item.children.length) {
                var $btn = $(TAG_BTN, {class: 'mmenu__button', text: 'Перейти'});
                $btn.data('items', item.children);
                $btn.data('title', striptags(item.text));
                $btn.appendTo($li);
            }

            $li.appendTo($ul);
        }

        $ul.appendTo($container);
        updateStyles();
    }

    var $menu      = $('#mobile-nav');
    var $menuBody  = $('.mobile-menu__body', $menu);
    var $container = $(TAG_DIV, {class: 'mobile-menu__container'});

    $container.appendTo($menuBody);

    $container.on('transitionend', function(event) {
        if ($(event.target).hasClass('mobile-menu__container')) {
            $container.removeClass('on');

            if (movedUp) {
                movedUp = false;
                openLevels.pop();
                $menu.find('.mmenu:last-child').remove();
                updateStyles();
                setTimeout(function() {
                    let translate = (100 / (openLevels.length + 1)) * -openLevels.length;
                    $container.css({transform: 'translateX(' + (translate) + '%)'});
                    animated = false;
                }, 1);
            } else {
                animated = false;
            }
        }
    });

    $menu.on('click', '.mmenu__button', function(e) {
        e.preventDefault();
        if (animated) return false;
        deepDive(
            $(e.target).data('items'),
            $(e.target).data('title'),
        );
    });

    $menu.on('click', '.mmenu__button2', function(e) {
        e.preventDefault();
        animated = true;
        movedUp  = true;

        var width     = (100 / (openLevels.length + 1));
        var translate = (100 / (openLevels.length + 1)) * -openLevels.length;

        $container.addClass('on');
        $container.css({
            transform: 'translateX(' + (translate + width) + '%)',
        });
    });

    $(document).on(App.EVENT_MMENU_OPEN, function(){
        updateStyles();
    });

    render(menulist);
})();

/**
 * Меню каталога
 */

(function() {
    var $navDropdown = $('.nav-catalog__dropdown');
    var stack        = [];

    $navDropdown.on('mouseenter', 'li', function(e) {
        var $menuItem   = $(this);
        var hasChildren = $menuItem.children('a').hasClass('hasChildren');
        var deepLevel   = parseInt($menuItem.data('level'));
        if (isNaN(deepLevel)) return;

        if (hasChildren && deepLevel >= 2) {
            var $nextLevel = $menuItem.children('ul');
            var h          = $nextLevel.height();
            stack.push($nextLevel);
            if ($navDropdown.height() < h) $navDropdown.height(h);
        }
    });

    $navDropdown.on('mouseleave', 'li', function(e) {
        var $menuItem   = $(this);
        var hasChildren = $menuItem.children('a').hasClass('hasChildren');
        var deepLevel   = parseInt($menuItem.data('level'));
        if (isNaN(deepLevel)) return;

        if (hasChildren && deepLevel >= 2) {
            stack.pop();
            $navDropdown.height('auto');
            for (var i = 0; i < stack.length; i++) {
                var h = stack[i].height();
                if ($navDropdown.height() < h) $navDropdown.height(h);
            }
        }
    });
})();

/*!
 * PipePrice
 * File: search.js
 * (c) 2021 delphinpro <delphinpro@yandex.ru>
 * licensed under the MIT license
 */

(function () {

    const elSearch = document.querySelector('.header .search');
    if (!elSearch) return;

    function expandableSearchBox() {
        elSearch.classList.add('_expandable');
        elSearch.classList.remove('_mobile');
    }

    function resetSearchBox() {
        elSearch.classList.remove('_expandable');
        elSearch.classList.remove('_mobile');
    }

    function makeMobileSearchBox() {
        elSearch.classList.add('_mobile');
        elSearch.classList.remove('_expandable');
    }

    function resetMobileSearchBox() {
    }

    App.mediaQueryManager.addListener(App.mq.XXS, makeMobileSearchBox);
    App.mediaQueryManager.addListener(App.mq.XS, makeMobileSearchBox);
    App.mediaQueryManager.addListener(App.mq.SM, makeMobileSearchBox);
    App.mediaQueryManager.addListener(App.mq.MD, expandableSearchBox);
    App.mediaQueryManager.addListener(App.mq.LG, expandableSearchBox);
    App.mediaQueryManager.addListener(App.mq.XL, resetSearchBox);
    App.mediaQueryManager.addListener(App.mq.WD, resetSearchBox);

})();

/*!
 * PipePrice
 * File: smenu.js
 * (c) 2019 delphinpro <delphinpro@gmail.com>
 * licensed under the MIT license
 */

(function () {

    $('.smenu__dropdown').on('click', function (e) {
        e.preventDefault();
        var isOpen = $(this).parent().hasClass('isOpen');
        if (!isOpen) {
            $(this).parent().addClass('isOpen');
        } else {
            $(this).parent().removeClass('isOpen');
        }
    });

})();

/**
 * Yandex карты
 *
 * Пример разметки:
 *
 * <div class="yandex-map"
 *      data-center="[55.950196568790176,37.95149399999996]"
 *      data-zoom="16"
 * ></div>
 */

(function() {
    var $yandexMaps = $('.yandex-map');
    if ($yandexMaps.length === 0) return;

    var defaults = {
        center: '[55.75161665120035, 37.61788699999992]',
        zoom  : 15,
    };

    ymaps.ready(function() {
        console.log('Yandex maps API is ready.');

        $yandexMaps.each(function(i, el) {
            var center = JSON.parse((el.dataset.center || defaults.center));
            var zoom   = JSON.parse((el.dataset.zoom || defaults.zoom));
            var point  = JSON.parse((el.dataset.point || JSON.stringify(center)));

            var myMap = new ymaps.Map(el, {
                center: center,
                zoom  : zoom,
            });

            myMap.behaviors.disable(['scrollZoom']);

            var myPlacemark = new ymaps.Placemark(point, {}, {
                iconLayout     : 'default#image',
                iconImageHref  : '/local/templates/bas/images/icons/point-brand.png',
                iconImageSize  : [51, 67],
                iconImageOffset: [-24, -67],
            });

            myMap.geoObjects.add(myPlacemark);
        });
    });
})();

